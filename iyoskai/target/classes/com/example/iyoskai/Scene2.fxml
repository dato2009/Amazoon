<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="466.0" prefWidth="671.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.iyoskai.Scene2">
   <children>
      <TextField fx:id="UsernameField" layoutX="14.0" layoutY="21.0" promptText="Username" />
      <TextField fx:id="PasswordField" layoutX="14.0" layoutY="73.0" promptText="Password" />
      <Text layoutX="14.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Gender" />
      <RadioButton fx:id="MaleButton" layoutX="28.0" layoutY="135.0" mnemonicParsing="false" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="GenderChoice" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="FemaleButton" layoutX="28.0" layoutY="162.0" mnemonicParsing="false" text="Female" toggleGroup="$GenderChoice" />
      <RadioButton fx:id="OtherButton" layoutX="28.0" layoutY="192.0" mnemonicParsing="false" text="Other" toggleGroup="$GenderChoice" />
      <ChoiceBox fx:id="CityChoice" layoutX="14.0" layoutY="229.0" prefWidth="150.0" />
      <CheckBox fx:id="TOACheckBox" layoutX="17.0" layoutY="296.0" mnemonicParsing="false" text="I accept terms of agreement" />
      <Button fx:id="SingUpButton" layoutX="17.0" layoutY="326.0" mnemonicParsing="false" onAction="#SignUp" text="Sign up" />
      <DatePicker fx:id="Picker" layoutX="204.0" layoutY="21.0" />
   </children>
</AnchorPane>
